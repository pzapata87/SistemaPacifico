    PageShowShadow = false,
    ShowExportDialog = false,
    Controller = ViewData["ControllerGetSnapshot"].ToString(),
    //Localization = "~/Content/Localization/es.xml",
    ServerTimeout = new TimeSpan(0, 10, 0),
